<dashboard hideEdit="true" theme="dark">
  <init>
    <unset token="and_nl"></unset>
    <set token="repeatEntity_tk">alone</set>
    <unset token="and_tk"></unset>
    <set token="help_tk">on</set>
  </init>
  <label>Setup</label>
  <search id="warnings">
    <query>| `check_rest_api` 
    </query>
  </search>
  <fieldset submitButton="false" autoRun="true">
  </fieldset>
  <row>
    <panel>
      <table>
        <search base="warnings">
          <query>fields - _time | fields - result | eval pass="CLICK HERE"</query>
        </search>
        <option name="drilldown">row</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <fields>["msg","host","port","user","pass"]</fields>
        <drilldown>
          <condition field="msg"></condition>
          <condition field="host">
            <link target="_blank">/manager/search/data/macros/oi_host?action=edit&amp;ns=openintelligence_nli_app&amp;pwnr=-&amp;app_only=1&amp;search=oi_&amp;count=25</link>
          </condition>
          <condition field="port">
            <link target="_blank">/manager/search/data/macros/oi_port?action=edit&amp;ns=openintelligence_nli_app&amp;pwnr=-&amp;app_only=1&amp;search=oi_&amp;count=25</link>
          </condition>
          <condition field="user">
            <link target="_blank">/manager/search/data/macros/oi_user?action=edit&amp;ns=openintelligence_nli_app&amp;pwnr=-&amp;app_only=1&amp;search=oi_&amp;count=25</link>
          </condition>
          <condition field="pass">
            <link target="_blank">/manager/search/data/macros/oi_pass?action=edit&amp;ns=openintelligence_nli_app&amp;pwnr=-&amp;app_only=1&amp;search=oi_&amp;count=25</link>
          </condition>
        </drilldown>
      </table>
    </panel>
  </row>
</dashboard>